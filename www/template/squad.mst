{{#squad}}
	<div class="rank">
		<div class="picto">
			<span>{{shirtNumber}}</span>
			<img src="images/{{position}}.svg" alt="t-shirt">
		</div>
		<div class="name">
			<p>{{name}}</p>
		</div>
		<div class="nationality">
			<img src="images/flags/{{nationality}}.png" alt="drapeau">
			<p>{{nationality}}</p>
		</div>
		<div class="birth">
			<p>{{dateOfBirth}} ({{age}})</p>
		</div>
		<div class="position">
			<p>{{position}}</p>
		</div>
	</div>
{{/squad}}